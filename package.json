{
  "name": "record",
  "version": "0.0.0",
  "description": "The official website for the record company",
  "repository": "https://github.com/Almo7aya/record.git",
  "author": "Ali Almohaya <alialmohaya@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
